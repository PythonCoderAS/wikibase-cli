    "postinstall": "./scripts/postinstall",
    "prepublishOnly": "npm run lint && npm test",
    "lint": "standard",
    "update-toc": "doctoc docs/*md",
    "docker:publish": "./scripts/docker_publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxlath/wikidata-cli.git"
  },
  "keywords": [
    "wikidata",
    "cli"
  ],
  "author": "maxlath",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxlath/wikidata-cli/issues"
  },
  "homepage": "https://github.com/maxlath/wikidata-cli#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "check-node-version": "^2.1.0",
    "commander": "git+https://github.com/maxlath/commander.js.git",
    "copy2cb": "^1.0.1",
    "execa": "^0.4.0",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "opn": "^4.0.2",
    "prompt": "^1.0.0",
    "shell-quote": "^1.6.1",
    "split": "^1.0.1",
    "through": "^2.3.8",
    "wikidata-edit": "^2.0.10",
    "wikidata-sdk": "^5.7.5"
  },
  "devDependencies": {
    "doctoc": "^1.3.0",
    "git-hooks": "^1.1.8",
    "mocha": "^4.0.1",
    "should": "^13.1.3",
    "standard": "^9.0.2"
  },
  "standard": {
    "ignore": [
      "dist"
