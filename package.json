{
  "name": "wikidata-cli",
  "version": "6.0.2",
  "description": "The command-line interface to Wikidata",
  "main": "index.js",
  "bin": {
    "wd": "./bin/wd",
    "wikidata-cli": "./bin/wikidata-cli"
  },
  "scripts": {
